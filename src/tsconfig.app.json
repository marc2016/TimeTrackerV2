{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "target": "es5",
    "types": [],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "typeorm": ["../node_modules/typeorm/typeorm-model-shim.js"]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
